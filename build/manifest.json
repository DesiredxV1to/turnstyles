{
  "name": "turnStyles for deepcut.fm",
  "version": "11.0.2",
  "description": "Apply tweaks to deepcut.fm - autobop, autodj, themes, styles, and more!",
  "manifest_version": 2,
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "content_scripts": [
    {
      "exclude_globs": [
        "*://turntable.fm/about",
        "*://turntable.fm/jobs",
        "*://turntable.fm/privacy",
        "*://turntable.fm/copyright",
        "*://turntable.fm/terms",
        "*://turntable.fm/static/*",
        "*://deepcuts.fm/about",
        "*://deepcuts.fm/jobs",
        "*://deepcuts.fm/privacy",
        "*://deepcuts.fm/copyright",
        "*://deepcuts.fm/terms",
        "*://deep-cuts.fm/static/*",
        "*://deep-cuts.fm/about",
        "*://deep-cuts.fm/jobs",
        "*://deep-cuts.fm/privacy",
        "*://deep-cuts.fm/copyright",
        "*://deep-cuts.fm/terms",
        "*://deep-cuts.fm/static/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end",
      "matches": [
        "*://turntable.fm/*",
        "*://deepcuts.fm/*",
        "*://deep-cuts.fm/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "turnStyles.css",
    "turnStyles.js",
    "themes/*",
    "styles/*"
  ]
}
